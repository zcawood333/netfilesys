{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server.js"
        },
        {
            "name": "Client GET",
            "program": "${workspaceFolder}/node_client/client.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "args": ["GET", "eea70be55a704722ae543d23125e0a53", "--debug"]
        },
        {
            "name": "Client GET (file doesn't exist)",
            "program": "${workspaceFolder}/node_client/client.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "args": ["GET", "0123456789abcdeffedcba9876543210", "--debug"]
        },
        {
            "name": "Client GET (multiple files)",
            "program": "${workspaceFolder}/node_client/client.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "args": ["GET", "eea70be55a704722ae543d23125e0a53", 
                            "17bf4d4e56b54732bbc5c4f8fc207213", 
                            "495f221a1e144d4691fdc12314d48ed0", 
                            "f356591135654b3e866594164b98f8f2", 
                            "f69e1bd7eb234ab7b34030127fef2e4b", "--debug"]
        },
        {
            "name": "Client GET (multiple files w/ bad request)",
            "program": "${workspaceFolder}/node_client/client.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "args": ["GET", "eea70be55a704722ae543d23125e0a53", 
                            "17bf4d4e56b54732bbc5c4f8fc207213", 
                            "495f221a1e144d4691fdc12314d48ed0", 
                            "0123456789abcdeffedcba9876543210",
                            "f356591135654b3e866594164b98f8f2", 
                            "f69e1bd7eb234ab7b34030127fef2e4b", 
                            "--debug"]
        },
        {
            "name": "Client PUT",
            "program": "${workspaceFolder}/node_client/client.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "args": ["PUT", "${workspaceFolder}/../test_files_to_upload/a.txt", "--debug"]
        },
        {
            "name": "Client POST",
            "program": "${workspaceFolder}/node_client/client.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "args": ["POST", "${workspaceFolder}/../test_files_to_upload/a.txt", "--debug"]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Client Upload",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_client/client.js",
            "args": ["--debug", "--method=POST", "--path=/upload", "--fpath=${workspaceFolder}/../test_files_to_upload/a.txt"]
        },
        {
            "name": "Client Download",
            "program": "${workspaceFolder}/node_client/client.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "args": ["--debug", "--path=/download/eea70be55a704722ae543d23125e0a53"]
        },
        {
            "name": "Client Multicast",
            "program": "${workspaceFolder}/node_client/client.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "args": ["--debug", "--multicast='this is a multicast message from the client'"]
        },        
    ]
}